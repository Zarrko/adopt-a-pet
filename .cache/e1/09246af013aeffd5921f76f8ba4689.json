 } else {\n      this.setState({ breeds: [] });\n    }\n  }\n  render() {\n    return (\n      <div>\n        <header>\n          <Link to=\"/\">Adopt Me!</Link>\n          <Link to=\"/search-params\">\n            <span aria-label=\"search\" role=\"img\">\n              🔍\n            </span>\n          </Link>\n        </header>\n        <Provider value={this.state}>\n          <Router>\n            <Results path=\"/\" />\n            <Details path=\"/details/:id\" />\n            <SearchParams path=\"/search-params\" />\n          </Router>\n        </Provider>\n      </div>\n    );\n  }\n}\n\n// Render App into the dom\nrender(React.createElement(App), document.getElementById(\"root\"));\n"},"lineCount":null}},"error":null,"hash":"dcabfed84e1fc4760e4883e08ef0d5e4","cacheData":{"env":{"API_KEY":"foXje6PyWOOYCovdAtYilRC086ds7HcQPMoW8nlidAAgSFKAQH","API_SECRET":"jNxrGEMrA4L4YFwtLtXUr7k1FLDNqwHFJgtbTiR1"}}}